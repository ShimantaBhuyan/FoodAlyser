ton {\r\n    color: #00000091 !important;\r\n    background-image: linear-gradient(147deg, #e1e9ea 0%, #9fb19c 74%) !important;\r\n    box-shadow: 0px 4px 32px rgb(39 34 34 / 40%) !important;\r\n}\r\n\r\n/* .recipeCard {\r\n    width: 300px;\r\n    max-width: 80vw;\r\n    min-height: 300px;\r\n    background: #e5de96 !important;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: center;\r\n    margin: 0 auto 10px auto !important;\r\n} */\r\n\r\n.title {\r\n    font-size: 25px;\r\n    text-align: center;\r\n}\r\n\r\n.recipeNameText {\r\n    font-weight: 600 !important;\r\n    font-size: 20px;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.servingSizeText {\r\n    font-weight: 300 !important;\r\n    font-size: 15px;\r\n}\r\n\r\n.recipesContainer {\r\n    display: grid;    \r\n    grid-template-columns: auto auto auto;\r\n    text-align: center;\r\n    /* flex-direction: row;\r\n    flex-wrap: wrap;\r\n    justify-content: space-around; */\r\n}\r\n\r\n.messageText {\r\n    font-size: 25px;\r\n    color: #000000a8;\r\n    font-weight: 600;\r\n    grid-column-start: 2;\r\n}\r\n\r\n#edamam-badge {\r\n    position: fixed;\r\n    bottom: 0;\r\n    right: -130px;\r\n    transition: right 1s;\r\n    background-color: #ffffff9d;\r\n    border-radius: 20px;\r\n    padding-left: 3px;\r\n}\r\n\r\n#iconsAttribution {   \r\n    font-size: 12px; \r\n    position: fixed;\r\n    bottom: 55px;\r\n    right: -176px;\r\n    transition: right 1s;\r\n    background-color: #0000002d;\r\n    border-radius: 6px;\r\n    padding-left: 3px;\r\n}\r\n\r\n#edamam-badge:hover, #iconsAttribution:hover {\r\n    right: 0;\r\n}\r\n\r\n.recipeDataContent {\r\n    transform: translateY(0) translateZ(0) !important;\r\n}\r\n\r\n.down {\r\n    transform: translateY(100%) translateY(5em) translateZ(0) !important;\r\n}\r\n\r\n.up {\r\n    transform: translateY(-105%) translateZ(0) !important;\r\n}\r\n\r\n.down, .up, .recipeDataContent {    \r\n    transition: transform 0.5s ease-out;\r\n}\r\n\r\n.up::before {      \r\n    position: absolute !important;\r\n    -webkit-filter: blur(5px) !important;\r\n    filter: blur(5px) !important;\r\n    background-size: cover !important;\r\n    opacity: 0.5 !important;\r\n}\r\n\r\n@media screen and (max-height:1024px){\r\n    .root, .MuiPaper-root, .MuiCard-root {\r\n        min-height: 25vh;\r\n        width: 40vw;\r\n    }\r\n    .title {\r\n        font-size: 25px;\r\n    }  \r\n    /* .recipesContainer {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n        max-width: 100vw;\r\n        overflow-x: scroll;\r\n    }\r\n    #headerText {\r\n        font-size: 25px !important;\r\n    } */\r\n}\r\n\r\n@media screen and (max-height:824px){\r\n    .root, .MuiPaper-root, .MuiCard-root {\r\n        min-width: 95vw;\r\n        min-height: 15vh;\r\n    }\r\n    .title {\r\n        font-size: 20px;\r\n        margin-top: 0;\r\n    }    \r\n    .recipesContainer {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n    }\r\n    #headerText {\r\n        font-size: 25px !important;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"15b3c9db7637b04489538c897798459b","cacheData":{"env":{}}}